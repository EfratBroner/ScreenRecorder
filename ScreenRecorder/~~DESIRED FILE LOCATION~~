<script>
    parentNodeId = '1057';
    $(function () {
        $('.searchSubmit2').on("click", function (e) {
            e.preventDefault();
            tabRelatedTo = "all";
            searchTerm = $('#mainSearchinner').val();
            sendAjax4();
            window.location.href = "/" + "search-results/?term=" + $('#mainSearch').val();

        });

        $('.searchSubmit3').on("click", function (e) {
            e.preventDefault();
            tabRelatedTo = "all";
            searchTerm = $('#MobileSearchinner').val();
            sendAjax4();
            window.location.href = "/" + "search-results/?term=" + $('#MobileSearchinner').val();

        });

        $('#mainSearch').keypress(function (e) {
            var key = e.which;
            if (key == 13)  // the enter key code
            {
                $('.searchSubmit').click();
            }
        });

        $('#MobileSearchinner').keypress(function (e) {
            var key = e.which;
            if (key == 13)  // the enter key code
            {
                $('.searchSubmit3').click();
            }
        });
        $("#header-search").on('shown.bs.collapse', function () {
            console.log("shown");
            $("#mainSearch").focus();
        });

    });

    $(function () {

        $('#chooseLanguage').on('changed.bs.select', function (e) {
            var test = $(this).find(":selected").data('link-to');
            window.location.href = test;

        });

    });


</script>                <a href="#reports" class="reportsTab" data-toggle="collapse" title="פתח דיווחי תנועה" role="button" aria-expanded="false" aria-controls="reports">
