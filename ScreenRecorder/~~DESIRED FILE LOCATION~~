<script>
    var trafficReportsPLayers = [];
    function initPlayer(file, image, title, elemntId) {

        //debugger;
        var trafficReportsPlayerInstance = jwplayer(elemntId).setup({
            file: file,
            image: image,
            title: title,
            height: 161,
            width: 245,
            "autostart": false,
            "displaytitle": true,
            //ga: '{}'
        });

        trafficReportsPlayerInstance.on('error', function (e) {
            if (e.message === 'The live stream is either down or has ended') {
                console.log('jwplayer error:  The live stream is either down or has ended');
            }
        });

        trafficReportsPLayers.push(trafficReportsPlayerInstance);
    }

