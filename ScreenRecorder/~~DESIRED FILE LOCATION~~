        $('div[id^=traffic-reports-player-]').each(function(i,elem){
            var $playerContainer = $(elem);
            if($playerContainer){
                var suffix = $playerContainer.data('camera-id');
                //console.log(suffix);
                var camera = cameras.filter(function(x){
                    return x.id == suffix;
                });
                // console.log(camera.image_file);
                if(camera){
                    var cameraFile = camera.player_url_web;
                    var cameraImgUrl = camera.image_file;
                    var cameraName = camera.title;
                    var playerContainerId = $playerContainer.attr('id');
                    initPlayer(cameraFile, cameraImgUrl, cameraName, playerContainerId);
                }
            }
        });


        var $searchInput = $('.searchBar .searchInput');
        $searchInput.on('keyup', function () {
            var text = $(this).val();
            var activeListElements = $('ul.active').find('li');
            if(activeListElements && activeListElements.length > 0){
                activeListElements.each(function(i,elem){

                    var road = $(elem).find('.road').text();
                    var area =  $(elem).find('.area').text();
                    var description = $(elem).find('.description').text();

                    if(text == ''){
                        $(this).show();
                    }else if(text != '' && (road.indexOf(text) != -1 || area.indexOf(text) != -1 || description.indexOf(text) != -1 )){
                        $(this).show();
                    }else{
                        $(this).hide();
                    }



                });
            }
            console.log(activeListElements);
        });


        var $stickyInfo = $('#reports');
        $stickyInfo.on('shown.bs.collapse', function () {
            $('.link.closeReports').focus();
        });
    });
</script>


